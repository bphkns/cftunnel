{
	"name": "cftunnel",
	"version": "0.1.0",
	"description": "Cloudflare Tunnel CLI â€” ngrok-like dev tunnels on your own domain",
	"type": "module",
	"bin": {
		"cftunnel": "./src/index.ts"
	},
	"files": [
		"src/",
		"README.md",
		"LICENSE"
	],
	"keywords": [
		"cloudflare",
		"tunnel",
		"ngrok",
		"dev-tunnel",
		"cloudflared",
		"cli"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/bphkns/cftunnel.git"
	},
	"engines": {
		"node": ">=18",
		"bun": ">=1.0"
	},
	"scripts": {
		"dev": "bun run src/index.ts",
		"build": "bun build ./src/index.ts --compile --outfile dist/cftunnel",
		"lint": "biome check src/",
		"lint:fix": "biome check --write src/",
		"format": "biome format --write src/",
		"check": "tsc --noEmit",
		"prepare": "husky",
		"prepublishOnly": "bun run lint && bun run check"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.0",
		"@types/bun": "^1.2.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"typescript": "^5.7.0"
	},
	"dependencies": {
		"@clack/prompts": "^1.0.0",
		"better-result": "^2.7.0",
		"picocolors": "^1.1.0"
	},
	"license": "MIT",
	"lint-staged": {
		"*.ts": "biome check --no-errors-on-unmatched"
	}
}
